<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>AServlet</servlet-name>
        <servlet-class>cn.devinkin.web.servlet.AServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>AServlet</servlet-name>
        <url-pattern>/AServlet</url-pattern>
    </servlet-mapping>
    

    <servlet>
        <servlet-name>BServlet</servlet-name>
        <servlet-class>cn.devinkin.web.servlet.BServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BServlet</servlet-name>
        <url-pattern>/BServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>cn.devinkin.web.servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WebServlet</servlet-name>
        <servlet-class>cn.devinkin.web.servlet.WebServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WebServlet</servlet-name>
        <url-pattern>/WebServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BookServlet</servlet-name>
        <servlet-class>cn.devinkin.web.servlet.BookServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BookServlet</servlet-name>
        <url-pattern>/BookServlet</url-pattern>
    </servlet-mapping>

    <listener>
        <listener-class>cn.devinkin.web.listener.IPListener</listener-class>
    </listener>
    <filter>
        <filter-name>AFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.AFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AFilter</filter-name>
        <url-pattern>/AServlet</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>BFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.BFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>BFilter</filter-name>
        <url-pattern>/AServlet</url-pattern>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>

    <filter>
        <filter-name>DFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.DFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>DFilter</filter-name>
        <url-pattern>/AServlet</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>CFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.CFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CFilter</filter-name>
        <url-pattern>/AServlet</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>IPFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.IPFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>IPFilter</filter-name>
        <url-pattern>/jsp/*</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>UserFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.UserFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>UserFilter</filter-name>
        <url-pattern>/users/*</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>AdminFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.AdminFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AdminFilter</filter-name>
        <url-pattern>/admin/*</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>EncodingFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.EncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <url-pattern>/webp/*</url-pattern>
        <url-pattern>/WebServlet</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>StaticFilter</filter-name>
        <filter-class>cn.devinkin.web.filter.StaticFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>StaticFilter</filter-name>
        <servlet-name>BookServlet</servlet-name>
    </filter-mapping>
</web-app>